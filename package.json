{
  "name": "stencil-tailwindcss-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-tailwindcss-components/stencil-tailwindcss-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --no-sandbox",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook.run": "start-storybook -p 6006 -s dist",
    "storybook": "npm-run-all --parallel build storybook.run",
    "build-storybook": "build-storybook -c ./.storybook -o ../public"
  },
  "dependencies": {
    "@stencil/core": "^2.7.0-0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@stencil/postcss": "^2.0.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-viewport": "^6.3.6",
    "@storybook/html": "^6.3.6",
    "@types/jest": "^26.0.21",
    "@types/node": "^16.4.3",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "case": "^1.6.3",
    "cssnano": "^5.0.7",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss-replace": "^1.1.3",
    "puppeteer": "^10.0.0",
    "tailwindcss": "^2.2.7"
  }
}
